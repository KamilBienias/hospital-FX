<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="700.0" minWidth="1200.0" prefHeight="700.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MainWindowController">
   <children>
      <Label fx:id="welcomeLabel" text="Welcome to our hospital" StackPane.alignment="TOP_LEFT">
         <StackPane.margin>
            <Insets left="10.0" right="10.0" top="50.0" />
         </StackPane.margin>
      </Label>
      <Button fx:id="showDoctorsButton" mnemonicParsing="false" onAction="#openWindowShowDoctors" text="Show doctors" StackPane.alignment="TOP_LEFT">
         <StackPane.margin>
            <Insets left="10.0" right="10.0" top="100.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="showPatientsButton" mnemonicParsing="false" onAction="#openWindowShowPatients" text="Show patients" StackPane.alignment="TOP_LEFT">
         <StackPane.margin>
            <Insets left="10.0" right="10.0" top="150.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
